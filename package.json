{
  "name": "Gepcash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --mode=developmentdebug",
    "android:dev-release": "react-native run-android --mode=developmentrelease",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme 'GepcashDev' --simulator 'iPhone 16 Pro Max'",
    "ios:prod": "react-native run-ios --scheme 'GepcashProd --simulator 'iPhone 16'",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "resetcache": "yarn start -- --reset-cache",
    "build:dev-apk": "cd android && ./gradlew assembleDevelopmentRelease && cd ..",
    "build:prod-apk": "cd android && ./gradlew assembleProductionRelease && cd ..",
    "build:dev-bundle": "cd android && ./gradlew bundleDevelopmentRelease && cd ..",
    "build:prod-bundle": "cd android && ./gradlew bundleProductionRelease && cd ..",
    "clean:gradle": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/elements": "^1.3.21",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "5.14.5",
    "@reduxjs/toolkit": "^1.9.6",
    "@types/qs": "^6.9.11",
    "@types/react-native-indicators": "^0.16.6",
    "@types/react-native-material-ripple": "^0.9.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.5",
    "immutable": "^5.0.2",
    "moment": "^2.30.1",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-config": "^1.5.1",
    "react-native-date-picker": "^5.0.8",
    "react-native-device-info": "^14.0.0",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-indicators": "^0.17.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-material-ripple": "^0.9.1",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "3.7.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.11.1",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-version-info": "^1.1.1",
    "react-persist": "^1.0.2",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "rn-wave-bottom-bar": "^2.2.36",
    "yup": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.87",
    "@react-native/eslint-config": "0.74.87",
    "@react-native/metro-config": "0.74.87",
    "@react-native/typescript-config": "0.74.87",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
